        },
    });

    // tab scroll

    var detailScroll = $("#wrap").offset();
    var reviewScroll = $("#review").offset();
    
    $(".btnLeft").on("click", function(){
        $("html, body").animate({scrollTop: detailScroll.top},400);
        $(this).addClass("selected");
